{
  "Main": {
    "uid": {
      "type": "UID",
      "config": {
        "label": "uid"
      }
    },
    "about_title": {
      "type": "StructuredText",
      "config": {
        "single": "heading1",
        "label": "about title",
        "placeholder": "Nada Debeaumont"
      }
    },
    "about_description": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph, em, hyperlink",
        "allowTargetBlank": true,
        "label": "about_description"
      }
    },
    "about_color": {
      "type": "Color",
      "config": {
        "label": "about color"
      }
    }
  },
  "SEO": {
    "meta_title": {
      "type": "Text",
      "config": {
        "label": "Meta Title",
        "placeholder": "Meta title..."
      }
    },
    "meta_description": {
      "type": "Text",
      "config": {
        "label": "Meta Description",
        "placeholder": "Meta Description..."
      }
    },
    "canonical": {
      "type": "Link",
      "config": {
        "select": "document",
        "customtypes": [
          "brewery",
          "country"
        ],
        "label": "Canonical URL",
        "placeholder": "Select the category to use in the canonical URL"
      }
    },
    "favicon": {
      "type": "Image",
      "config": {
        "constraint": {
          "width": 96,
          "height": 96
        },
        "thumbnails": [],
        "label": "favicon"
      }
    }
  }
}